{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"target": "ES5",
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "./lib/cjs",
		"strict": true,
		"noEmit": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true
	},
	"exclude": [
		"./**/**cli**"
	]
}
