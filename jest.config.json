{
	"preset": "ts-jest",
	"testRegex": "(/tests/.*(test|spec))\\.tsx?$",
	"extensionsToTreatAsEsm": [
		".ts"
	],
	"transform": {},
	"coverageThreshold": {
		"global": {
			"branches": 80,
			"functions": 80,
			"lines": 80,
			"statements": 80
		}
	},
	"collectCoverage": true,
	"coverageReporters": ["text", "cobertura"],
	"collectCoverageFrom": ["./src/**/*.ts", "!**/lib/**", "!**/tests/**"]
}
